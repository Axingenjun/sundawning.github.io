{
  "manifest_version": 3,
  "name": "chrome-extensions-syncLocalStorage",
  "version": "0.0.1",
  "permissions": ["storage"],
  "action": {
    "default_title": "LocalStorage同步",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
